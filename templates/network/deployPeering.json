{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "peer": {
      "type": "object"
    },
    "vnetName": {
      "type": "string"
    },
    "remoteVnetName": {
      "type": "string"
    }
  },
  "resources": [],
  "outputs": {
    "vnet": {
      "type": "object",
      "value": "[parameters('vnet')]"
    },
    "PeerCount": {
      "type": "int",
      "value": "[parameters('vnet').peerCount]"
    }
  }
}