{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "singleVnetObject": {
            "type": "object"
        },
        "nameDefaults": {
            "type": "object"
        },
        "baseUrl": {
            "type": "string"
        }
    },
    "variables": {
        "vnetBaseName": "[parameters('nameDefaults').vnetBaseName]",
        "vnetName": "[concat(variables('vnetBaseName'), parameters('singleVnetObject').name)]",
        "vnetAddressSpace": "[parameters('singleVnetObject').addressSpace]",
        "subnetCopyObject": {
            "copy": [
                {
                    "name": "subnetCopy",
                    "count": "[parameters('singleVnetObject').subnetCount]",
                    "input": {
                        "name": "[parameters('singleVnetObject').subnet[string(copyIndex('subnetCopy'))].name]",
                        "properties": {
                            "addressPrefix": "[parameters('singleVnetObject').subnet[string(copyIndex('subnetCopy'))].network]"
                        }
                    }
                }
            ]
        }
    },
    "resources": [
    ],
    "outputs": {
        "exampleObject": {
            "value": "[variables('subnetCopyObject')]",
            "type": "object"
        },
    "exampleArrayOnObject": {
      "value": "[variables('subnetCopyObject').subnetCopy]",
      "type" : "array"
    },
    "exampleArray": {
      "value": "[variables('subnetCopyObject')]",
      "type" : "array"
    }
  }
}