{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "storageAccountName": {
            "type": "string",
            "defaultValue": "tempal"
        }
    },
    "resources": [
        {
            "apiVersion": "2017-10-01",
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "name": "",
            "properties": {
                "addressPrefix": "[parameters('singleVnetObject').subnet[string(copyIndex('subnetCopy'))].network]"
            },
            "copy": {
                "name": "subnetCopy",
                "count": "[parameters('singleVnetObject').subnetCount]",
                "mode": "Serial"
            }
        }
    ]
}